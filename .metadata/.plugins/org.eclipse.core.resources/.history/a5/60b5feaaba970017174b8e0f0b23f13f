<xs:attribute name="tvschedule" type="xs:string" />

<xs:element name="tvschedule">
	<xs:complexType>
		<xs:all>
		<xs:element name="channel">
			<xs:complexType>
				<xs:all>
					<xs:element name="banner" type="xs:string"/>
					<!-- banner end  -->
					
					<xs:element name="day">
					<xs:complexType>
						<xs:all>
						<xs:element name="date" type="xs:string"/>
						<xs:element name="slot">
							<xs:complexType>
								<xs:choice>
									<xs:element name="holiday" type="xs:string"/>
									<xs:element name="programslot">
									<xs:complexType>
									<xs:all>
									<xs:element name="time" type="xs:string"/>
									<xs:element name="title" type="xs:string"/>
									<xs:element name="description" type="xs:string" minOccurs="0"/>
									</xs:all>
									</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						</xs:all>
					</xs:complexType>
					</xs:element>
					<!-- day end  -->
					<xs:element name="date" type="xs:string"/>	
				</xs:all>
			</xs:complexType>
		</xs:element>
		</xs:all>
	</xs:complexType>
</xs:element>



